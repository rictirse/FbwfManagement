<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="DriverLetter">磁碟機代號</sys:String>
    <sys:String x:Key="CacheSize">快取大小</sys:String>
    <sys:String x:Key="FbwfInstall">Fbwf安裝</sys:String>
    <sys:String x:Key="FbwfEnable">Fbwf啟用</sys:String>
    <sys:String x:Key="OverlayCacheDataCompression">壓縮快取記憶體</sys:String>
    <sys:String x:Key="OverlayCachePreAllocation">預載快取大小</sys:String>
    <sys:String x:Key="SizeDisplay">磁碟大小設定為記憶體大小顯示</sys:String>
    <sys:String x:Key="AutoMountAtBoot">開機自動掛載</sys:String>
    <sys:String x:Key="FbwfRamDisk">Fbwf dynamic ramdisk</sys:String>
    <sys:String x:Key="NeedReboot">*需重開機</sys:String>
    <sys:String x:Key="Apply">套用</sys:String>

    <!--MessageBox-->
    <sys:String x:Key="LoadConfigErr" xml:space="preserve">讀取設定檔失敗&#x0a;錯誤訊息：&#x0a;</sys:String>
    <sys:String x:Key="SaveConfigErr" xml:space="preserve">寫入設定檔失敗&#x0a;錯誤訊息：&#x0a;</sys:String>
</ResourceDictionary>